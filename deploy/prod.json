{
  "apiVersion": "nais.io/v1alpha1",
  "kind": "Application",
  "metadata": {
    "labels": {
      "team": "tbd"
    },
    "name": "spole",
    "namespace": "default"
  },
  "spec": {
    "env": [],
    "image": "${DOCKER_TAG}",
    "ingresses": [
      "https://spole.nais.adeo.no"
    ],
    "liveness": {
      "failureThreshold": 10,
      "initialDelay": 20,
      "path": "/isalive",
      "periodSeconds": 5,
      "timeout": 1
    },
    "port": 8080,
    "prometheus": {
      "enabled": true,
      "path": "/internal/metrics"
    },
    "readiness": {
      "initialDelay": 20,
      "path": "/isready",
      "timeout": 1
    },
    "replicas": {
      "cpuThresholdPercentage": 90,
      "max": 2,
      "min": 2
    },
    "resources": {
      "limits": {
        "cpu": "2000m",
        "memory": "1024Mi"
      },
      "requests": {
        "cpu": "1000m",
        "memory": "256Mi"
      }
    },
    "vault": {
      "enabled": true
    },
    "webproxy": true,
    "team": "tbd"
  }
}
