{
  "apiVersion": "nais.io/v1alpha1",
  "kind": "Application",
  "metadata": {
    "labels": {
      "team": "tbd"
    },
    "name": "spole",
    "namespace": "default"
  },
  "spec": {
    "image": "set_from_pipe",
    "ingresses": [
      "https://spole.nais.preprod.local"
    ],
    "liveness": {
      "failureThreshold": 10,
      "initialDelay": 20,
      "path": "/isalive",
      "periodSeconds": 5,
      "timeout": 1
    },
    "port": 8082,
    "prometheus": {
      "enabled": true,
      "path": "/internal/metrics"
    },
    "readiness": {
      "initialDelay": 20,
      "path": "/isready",
      "timeout": 1
    },
    "replicas": {
      "cpuThresholdPercentage": 90,
      "max": 1,
      "min": 1
    },
    "resources": {
      "limits": {
        "cpu": "2000m",
        "memory": "512Mi"
      },
      "requests": {
        "cpu": "1000m",
        "memory": "256Mi"
      }
    },
    "team": "tbd",
    "vault": {
        "enabled": true,
        "paths": [
          {
            "mountPath": "/var/run/secrets/nais.io/vault",
            "kvPath": "/kv/preprod/fss/spole/default"
          },
          {
            "mountPath": "/var/run/secrets/nais.io/azure",
            "kvPath": "/azuread/data/dev/creds/helse-spole"
          }
        ]
    },
    "webproxy": true,
    "secureLogs": {
      "enabled": true
    },
    "env": [
      {
        "name": "AKTORREGISTER_URL",
        "value": "https://app-q1.adeo.no/aktoerregister"
      }
    ]
  }
}
